extends layout

block content
  h1= title
  p You will face many defeats in life, but never let yourself be defeated
  button(type='button' class='btn btn-primary' onclick="update()") Press Me
  div(id="target") Display
    script.
      var client_count = 0
      function update(){
        client_count ++;
        document.getElementById("target").innerHTML = `Value now ${client_count}`
      }
    